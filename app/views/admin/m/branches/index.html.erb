<p id="notice"><%= notice %></p>

<h1>Listing M Branches</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m_branches.each do |m_branch| %>
      <tr>
        <td><%= m_branch.name %></td>
        <td><%= link_to 'Show', admin_m_branches_path(m_branch) %></td>
        <td><%= link_to 'Edit', edit_admin_m_branch_path(m_branch) %></td>
        <td><%= link_to 'Destroy', admin_m_branch_path(m_branch), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New M branch', new_admin_m_branch_path %>
